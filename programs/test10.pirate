pirate.sail()() = {
    pirate.write_cell(pirate.integer.1)(increment)
    pirate.write_cell(pirate.integer.0)(min)
    pirate.copy(min)(current)
    pirate.write_cell(pirate.integer.10)(max)
    pirate.write_cell(pirate.integer.10)(new_line)
    pirate.write_cell(@offset)(o)

    @offset

    pirate.print_cell_as_number(current)()
    pirate.print_cell_as_character(new_line)()

    pirate.integer_add(current increment)(current)

    pirate.integer_within_range(min current max)(within_range)

    pirate.jump_if(within_range o)()
}
